var fullname,mobile,username;exports.checkAuth=function(e){var t=Ti.App.Properties.getString("user_id");if(""==t||null==t){var i=Alloy.createController("auth/login").getView();i.open()}else Ti.App.fireEvent("home:refresh"),e&&e()},exports.updateProfile=function(){},exports.logout=function(e){Ti.App.Properties.setString("user_id",""),Ti.App.Properties.setString("fullname",""),console.log("start callback"),e&&e()};