function openWindow(e){if("android"==Ti.Platform.osname)e.open();else{var t=Alloy.Globals.navMenu;t.openWindow(e,{animated:!0})}}function removeAllChildren(e){for(var t=e.children.slice(0),i=0;i<t.length;++i)e.remove(t[i])}function dialogTextfield(e){var t=Ti.UI.createTextField(),i=Ti.UI.createAlertDialog({title:"Enter Point",androidView:t,style:Ti.UI.iPhone.AlertDialogStyle.PLAIN_TEXT_INPUT,buttonNames:["OK","cancel"]});i.addEventListener("click",function(t){var i=t.text;console.log("bidding point"+i),e(i)}),i.show()}function createAlert(e,t,i){var o=Titanium.UI.createAlertDialog({title:e,ok:"OK",message:t});o.show(),o.addEventListener("click",function(e){console.log(e.index+" "+e.source.ok),0==e.index&&(console.log(typeof i),"function"==typeof i&&i&&i())})}var mainView=null;exports.construct=function(e){mainView=e},exports.deconstruct=function(){mainView=null},exports.closeWindow=function(e){if("android"==Ti.Platform.osname)e.close();else{var t=Alloy.Globals.navMenu;t.closeWindow(e,{animated:!0})}},exports.openWindow=_.throttle(openWindow,500,!0),exports.removeAllChildren=_.debounce(removeAllChildren,0,!0),exports.createAlert=_.throttle(createAlert,500,!0),exports.dialogTextfield=_.throttle(dialogTextfield,500,!0),exports.now=function(){var e=new Date,t=e.getDate(),i=e.getMonth()+1,o=e.getFullYear(),a=e.getHours(),r=e.getMinutes(),l=e.getSeconds();return 10>r&&(r="0"+r),10>l&&(l="0"+l),10>t&&(t="0"+t),10>i&&(i="0"+i),datetime=o+"-"+i+"-"+t+" "+a+":"+r+":"+l,datetime};